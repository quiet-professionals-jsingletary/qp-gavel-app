{
  "name": "qp-ampd-app",
  "version": "1.0.0",
  "author": "James Singletary IV",
  "description": "ExpressJS RESTful API Server built on top of RESTful API serving up data for GAVEL",
  "main": "server.js",
  "homepage": "./client/build/index.html",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "sloc": "^0.2.1"
  },
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npm run start --prefix client",
    "client:prep": "cd client && npm install && npm run client:build",
    "client:build": "npm run client:copy && react-scripts build",
    "client:copy": "ncp ./client/node_modules/@arcgis/core/assets ./client/public/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quiet-Professionals-LLC/qp-ampd-app.git"
  },
  "keywords": [
    "GAVEL",
    "Mobile Phone Data",
    "QP",
    "Quiet Professionals LLC",
    "Venntel"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quiet-Professionals-LLC/qp-ampd-app/issues"
  }
}
